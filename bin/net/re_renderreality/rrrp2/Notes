Text Formatting:

Colored Text: {
	Text coloredText = Texts.builder("Golden text").color(TextColors.GOLD).build();
	Text styledText = Texts.builder("Styled text").style(TextStyles.ITALIC).build();
	Text multiColoredText = Texts.builder("Multi-").color(TextColors.YELLOW).append(Texts.builder("colored text").color(TextColors.RED).build()).build();
	Text clickableText = Texts.builder("Click here!").onClick(TextActions.runCommand("Popup text")).build();
	Text adventurers = Texts.builder("These players are in adventure mode: ").append(Texts.of(Selectors.parse("@a[m=2]"))).build();
}